{
  "name": "imgsort",
  "version": "1.0.0",
  "description": "This utility takes all image files from a folder and reorganizes them into date based folder tree structure.",
  "main": "index.js",
  "dependencies": {
    "exif": "^0.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/exif": "^0.6.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "sort": "npm run build && npm run exec-index",
    "revert": "npm run build && npm run exec-revert",
    "exec-index": "node dist/index.js",
    "exec-revert": "node dist/revert.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn index.ts",
    "test": "mocha -r ts-node/register tests/**/*.tests.ts"
  },
  "author": "ZigaBobnar",
  "license": "MIT"
}
